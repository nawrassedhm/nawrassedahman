{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "components/*": ["components/*"],
      "components": ["components"],
      "layout/*": ["layout/*"],
      "layout": ["layout"],
      "utils/*": ["utils/*"],
      "providers/*": ["providers/*"],
      "images/*": ["images/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "analytics.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
